{
    "data_setups":{
        "root": "./data",
        "dataset_name": "cifar10",
        "batch_size": 50,
        "n_clients": 100,
        "partition": {
            "method": "lda",
            "alpha": 0.3
        }
    },
    "train_setups":{
        "algo": {
            "name": "fedetf",
            "params": {}
        },
        "scenario": {
            "n_rounds": 320,
            "sample_ratio": 0.1,
            "local_epochs": 10,
            "device": "cuda:0"
        },
        "model":{
            "name": "vgg_etf",
            "params": {}
        },
        "optimizer":{
            "params": {"lr": 0.05, "momentum":0.9, "weight_decay": 1e-5}
        },
        "scheduler":{
            "enabled": true,
            "name": "multistep",
            "params": {"gamma":0.1, "milestones": [160, 240]}
        },
        "seed": 1000
    },
    "wandb_setups":{
        "project": "NeurIPS_FedDR",
        "group": "fedetf",
        "name": "fedetf"
    }
}
